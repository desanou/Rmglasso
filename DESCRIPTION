Package: mglasso
Type: Package
Title: Multiscale Graphical Lasso
Version: 0.1.0
Authors@R: c(person("Edmond", "Sanou", 
                    email = "doedmond.sanou@univ-evry.fr", role = c("aut", "cre")),
